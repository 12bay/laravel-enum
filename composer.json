{
    "name": "mad-web/:package_name",
    "description": ":package_description",
    "type": "library",
    "keywords": [
        "mad-web",
        ":package_name"
    ],
    "homepage": "https://github.com/mad-web/:package_name",
    "license": "MIT",
    "authors": [
        {
            "name": ":author_name",
            "email": "madweb.deb@gmail.com",
            "homepage": "https://github.com/mad-web",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "~5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "orchestra/testbench": "~3.4.8",
        "mockery/mockery": "^0.9.9"
    },
    "autoload": {
        "psr-4": {
            "MadWeb\\Skeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MadWeb\\Skeleton\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MadWeb\\Skeleton\\SkeletonServiceProvider"
            ],
            "aliases": {
                "Skeleton": "MadWeb\\Skeleton\\SkeletonFacade"
            }
        }
    }
}
